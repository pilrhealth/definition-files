{
    "name": "UMN News Data Handler",
    "code": "pilrhealth:mobile:news_handler",
    "version": 1,
    "description": "A CSV data file output for SUP News",
    "definition_type": "file_handler",
    "dataset": "pilrhealth:mobile:umn_news",
    "dataset_version": 1,
    "parser": {
        "parse_by": "column_name",
        "col_name_row": 1,
        "skip_rows": 1,
        "null_expressions": ["NaN", "Inf", ""],
        "date_format": "yyyy-MM-dd'T'HH:mm:ss",
        "data_mappings": [
            {
                "from_name": "id",
                "to_name": "id"
            },
            {
                "from_name": "language",
                "to_name": "language"
            },
            {
                "from_name": "category",
                "to_name": "category"
            },
            {
                "from_name": "day",
                "to_name": "day"
            },{
                "from_name": "title",
                "to_name": "title"
            },
            {
                "from_name": "text",
                "to_name": "text"
            },{
                "from_name": "website_link",
                "to_name": "website_link"
            },{
                "from_name": "website_link_text",
                "to_name": "website_link_text"
            },{
                "from_name": "video_link",
                "to_name": "video_link"
            },{
                "from_name": "video_link_text",
                "to_name": "video_link_text"
            },{
                "from_name": "image_url",
                "to_name": "image_url"
            }

        ],
        "metadata_mappings": [
            {
                "from_name": "setting_code",
                "to_name": "setting_code"
            }
        ]
    }
}
